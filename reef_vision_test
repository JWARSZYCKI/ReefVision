
#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'Reef_Vision==0.0.3','console_scripts','edgetpu_detect_server'
__requires__ = 'Reef_Vision==0.0.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Reef_Vision==0.0.3', 'console_scripts', 'reef_detect_server')()
    )



